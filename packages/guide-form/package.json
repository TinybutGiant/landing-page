{
  "name": "@replit/guide-form",
  "version": "1.0.0",
  "description": "Shared guide application form component",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "dev:tsc": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "form",
    "guide",
    "application"
  ],
  "author": "Replit",
  "license": "MIT",
  "dependencies": {
    "html2pdf.js": "^0.12.0"
  },
  "peerDependencies": {
    "@hookform/resolvers": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.0.0",
    "zod": "^3.0.0",
    "lucide-react": "^0.400.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/html2pdf.js": "^0.10.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
